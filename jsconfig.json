{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": "src",
    "paths": {
      "components/*": [
        "./src/components/*"
      ],
      "hooks/*": [
        "./src/hooks/*"
      ],
      "redux/*": [
        "./src/redux/*"
      ],
      "screens/*": [
        "./src/screens/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}